         </children>
      </VBox>
      <VBox alignment="TOP_CENTER" layoutX="1895.0" layoutY="10.0">
         <children>
             <Label contentDisplay="CENTER" text="Saturday">
                 <padding>
                     <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                 </padding>
             </Label>
        <ListView fx:id="saturdayListView" />
         </children>
      </VBox>
      <VBox alignment="TOP_CENTER" layoutX="1995.0" layoutY="10.0">
         <children>
             <Label contentDisplay="CENTER" text="Sunday">
                 <padding>
