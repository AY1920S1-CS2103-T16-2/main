<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.image.ImageView?>

<StackPane prefHeight="23.0" prefWidth="100.0" styleClass="stack-pane" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <HBox prefHeight="20.0" prefWidth="90.0">
      <ImageView xmlns:fx="http://javafx.com/fxml/1" fx:id="badgeImage" fitHeight="100.0" fitWidth="100.0"
                 pickOnBounds="true"
                 preserveRatio="true" xmlns="http://javafx.com/javafx/8"/>
      <children>
         <Label fx:id="achievementTitle" prefHeight="23.0" styleClass="cap-result-display" />
      </children>
   </HBox>
</StackPane>
